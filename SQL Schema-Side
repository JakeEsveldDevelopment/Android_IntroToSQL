CREATE TABLE players
(
  id INTEGER PRIMARY KEY,
  first_name TEXT,
  last_name TEXT,
  age INTEGER
);

INSERT INTO players
	(first_name, last_name, age)
VALUES
	('Jeff', 'Ryan', 32);
    
